<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/io/github/ellie_commons/indicator-simplethings/" id="io.github.ellie_commons.indicator-simplethings">
	<key name="text" type="s">
		<default>""</default>
		<summary>Text to display</summary>
		<description>Text to display in wingpanel. Empty string is replaced by a localized default</description>
	</key>
	<!-- Values: 0(Left), 1(Center), 2(Right) -->
	<key name="position" type="i">
	      	<default>0</default>
		<range min="0" max="2"/>
	</key>
	</schema>
</schemalist>
